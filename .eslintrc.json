{
  "extends": "next/core-web-vitals",

  "rules": {
    "no-console": "off", // Disables the rule that warns about using console.log
    "react/react-in-jsx-scope": "off", // Disables the rule requiring React to be in scope
    "no-unused-vars": "warn", // Changes unused variable errors to warnings
    "import/no-anonymous-default-export": "off"
  }
}
